    "topthink/think-multi-app": "^1.0",
    "topthink/think-ide-helper": "^1.0",
    "topthink/think-throttle": "^1.3",
    "topthink/think-filesystem": "^2.0",
    "doctrine/annotations": "^1.14",
    "ramsey/uuid": "^4.2",
    "phpoffice/phpspreadsheet": "^1.28",
    "funadmin/fun-addons": "^5.1.10"
